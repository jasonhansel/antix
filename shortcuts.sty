



\newcommand{\dfff}{\mathrm{d}}
\newcommand{\dfs}{{\!\hiderel{\wedge}\!\dfff}}

\RequirePackage{gtl}
\ExplSyntaxOn
\tl_new:N \l_demo_tl
\seq_new:N \l_seq
\cs_new:Npn \dff #1 {
    \tl_set:Nn \l_demo_tl {#1}
    \seq_gclear:c {l_seq}
    \regex_extract_all:nnNTF {(?:.(?:_.|_\{.*?\}|)|\{.*?\{.*?\}.*?\})\s*} {#1} \l_seq {
        \dfff\seq_use:Nnnn \l_seq {\!\hiderel{\wedge}\!\dfff}{\!\hiderel{\wedge}\!\dfff}{\!\hiderel{\wedge}\!\dfff}
    }{fail}
%}
%    \regex_extract_all:nnNTF {(?:.(?:_.|_\{.*?\})|\{.*?\})\s*} {#1} \l_seq {
%        \dfff\seq_use:Nnnn \l_seq {\!\hiderel{\wedge}\!\dfff}{\!\hiderel{\wedge}\!\dfff}{\!\hiderel{\wedge}\!\dfff}
%    }{fail}
}
\ExplSyntaxOff


% Intersperse one string in another
% TODO handle more complex subscripts and superscripts
\ExplSyntaxOn
\cs_new:Npn \inter #1 #2 {
    \tl_set:Nn \l_demo_tl {#2}
    \seq_gclear:c {l_seq}
    \regex_extract_all:nnNTF {.(?:\c[O][_^](?:.|\c{.*})|')*\s*} {#2} \l_seq {
        \seq_use:Nnnn \l_seq {#1}{#1}{#1}
    }{fail}
}
\ExplSyntaxOff


